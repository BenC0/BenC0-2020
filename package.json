{
  "name": "benc0-2020",
  "version": "1.0.0",
  "description": "BenC0 site 2020 edition",
  "scripts": {
    "build": "set enviro=production&& eleventy",
    "watch": "set enviro=local&& eleventy --watch",
    "scss": "node-sass --watch \"./scss\" -o \"./css\"",
    "serve": "set enviro=local&& concurrently \"npm run scss\" \"eleventy --serve\"",
    "debug": "set enviro=production&& DEBUG=* eleventy"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/BenC0/BenC0-2020"
  },
  "author": {
    "name": "Ben Co",
    "email": "suddensquirrel94@gmail.com",
    "url": "https://bencro.netlify.com/"
  },
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^0.10.0",
    "@11ty/eleventy-navigation": "^0.1.3",
    "@11ty/eleventy-plugin-rss": "^1.0.7",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.1",
    "luxon": "^1.21.3",
    "markdown-it": "^8.4.2",
    "markdown-it-anchor": "^5.2.5"
  },
  "dependencies": {
    "concurrently": "^5.1.0",
    "fs-extra": "^8.1.0",
    "node-sass": "^4.13.1",
    "path": "^0.12.7",
    "sass": "^1.25.0"
  }
