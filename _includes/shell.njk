<!doctype html>
<html lang="en">
<head>
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<title>{{ renderData.title or title or metadata.title }}</title>
	<meta name="Description" content="{{ renderData.description or description or metadata.description }}">
	<meta name="theme-color" content="#011627">
	{% block styles %}
		<link rel="stylesheet" href="{{ '/css/shell.css' | url }}">
		<link href="https://fonts.googleapis.com/css?family=Comfortaa:300|Fredoka+One&display=swap" rel="stylesheet">
	{% endblock %}
</head>
<body nav="inactive" theme="night" page="{% block body_page_attr %} home {% endblock %}">
	{% include "./modules/header.njk" %}
	<section id="main">
		{% block content %}
			<div class="content">
				<h1>404</h1>
			</div>
		{% endblock %}
	</section>
	{% include "./modules/menu.njk" %}
	<script src="{{ '/js/index.js' | url }}"></script>
</body>
</html>
